{
  "name": "demo-axum-hotwire-tailwind",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build": "run-s build:preclean build:tailwind parcel:prepjs parcel:prephtml build:parcel build:rustprep build:postclean",
    "build:preclean": "rm -f ./public/*.js && rm -f ./public/*.css && rm -f ./public/*.map && rm -f templates/base.html",
    "build:tailwind": "tailwindcss -i ./src/frontend/main.css -o ./build/main.css",
    "parcel:prepjs": "rsync -a --prune-empty-dirs --include '*/' --include '*.js' --exclude '*' ./src/frontend/ ./build/",
    "build:parcel": "parcel build ./build/base.html --public-url=/dist --dist-dir=./public/",
    "parcel:prephtml": "cp ./src/frontend/base.html ./build/",
    "build:rustprep": "mv ./public/base.html ./templates/",
    "build:postclean": "rm -rf ./build"
  },
  "author": "",
  "devDependencies": {
    "@github/combobox-nav": "2.1.5",
    "@hotwired/stimulus": "3.2.1",
    "@hotwired/turbo": "7.2.4",
    "@tailwindcss/forms": "0.5.3",
    "lodash.debounce": "4.0.8",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.3",
    "tailwindcss": "3.2.4"
  },
  "dependencies": {
    "g": "^2.0.1",
    "typescript": "^4.9.4"
  }
}
