{
  "name": "demo-axum-hotwire-tailwind",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "scripts": {
    "build": "run-s build:preclean build:tailwind parcel:prepjs parcel:prephtml build:parcel build:rustprep",
    "build:preclean": "mkdir -p target/ui/build && mkdir -p target/ui/public && rm -f target/ui/public/* && rm -f templates/base.html",
    "build:tailwind": "tailwindcss -i ui/src/main.css -o target/ui/build/main.css",
    "parcel:prepjs": "rsync -a --prune-empty-dirs --include '*/' --include '*.js' --include '*.ico' --exclude '*' ui/src/ target/ui/build",
    "build:parcel": "parcel build target/ui/build/base.html --public-url=/dist --dist-dir=target/ui/public/",
    "parcel:prephtml": "cp  ui/src/base.html target/ui/build/",
    "build:rustprep": "cp target/ui/public/base.html ui/templates/"
  },
  "devDependencies": {
    "@github/combobox-nav": "2.1.7",
    "@hotwired/stimulus": "3.2.1",
    "@hotwired/turbo": "7.3.0",
    "@tailwindcss/forms": "0.5.3",
    "lodash.debounce": "4.0.8",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.3",
    "tailwindcss": "3.2.7"
  },
  "dependencies": {
    "typescript": "^5.0"
  }
}
